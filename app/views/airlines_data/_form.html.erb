<%= form_tag("/airlines_datas", method: "post") do %>
  <%= label_tag(:airlines_name, "Airlines Name:") %>
  <%= text_field_tag(:airlines_name) %>

	<%= label_tag(:airlines_address, "Airlines Address:") %>
  <%= text_field_tag(:airlines_address) %>

  <%= label_tag(:airlines_contact, "Airlines Contact:") %>
  <%= text_field_tag(:airlines_contact) %>

  <%= label_tag(:airlines_plane_capacity, "Airlines Plane Capacity:") %>
  <%= text_field_tag(:airlines_plane_capacity) %>


  <%= submit_tag("Add") %>
  <% end %>